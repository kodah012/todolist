{
  "name": "todolist",
  "version": "1.0.0",
  "scripts": {
    "init": "npm install --prefix frontend",
    "dev": "concurrently 'npm run dev-backend' 'npm run dev-frontend'",
    "dev-backend": "cargo run --manifest-path backend/Cargo.toml",
    "dev-frontend": "npm run dev --prefix frontend"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
